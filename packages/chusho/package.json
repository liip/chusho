{
  "name": "chusho",
  "version": "0.1.0-alpha.0",
  "license": "MIT",
  "description": "A library of bare accessible Vue components and tools.",
  "keywords": [
    "vue",
    "components",
    "accessibility",
    "wcag"
  ],
  "author": {
    "name": "Liip",
    "url": "https://www.liip.ch/"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/liip/chusho.git",
    "directory": "packages/chusho"
  },
  "bugs": {
    "url": "https://github.com/liip/chusho/issues"
  },
  "homepage": "https://www.chusho.dev/",
  "scripts": {
    "start": "npm run storybook:serve",
    "storybook:build": "vue-cli-service storybook:build -c config/storybook -s public -o docs --docs",
    "storybook:serve": "vue-cli-service storybook:serve -p 6006 -c config/storybook -s public --ci",
    "storybook:build:debug": "npm run storybook:build -- --debug-webpack",
    "format": "vue-cli-service lint",
    "validate": "vue-cli-service lint --no-fix",
    "test:unit": "vue-cli-service test:unit",
    "test:unit:dev": "vue-cli-service test:unit --watch",
    "test:unit:debug": "node --inspect-brk ./node_modules/jest/bin/jest.js --runInBand",
    "test": "npm run validate && npm run test:unit",
    "build": "rollup -c",
    "prepublishOnly": "npm run build"
  },
  "main": "dist/chusho.js",
  "umd:main": "dist/chusho.umd.js",
  "module": "dist/chusho.module.js",
  "typings": "dist/chusho.d.ts",
  "sideEffects": false,
  "files": [
    "dist/"
  ],
  "dependencies": {
    "ts-essentials": "^6.0.4"
  },
  "devDependencies": {
    "@storybook/addon-a11y": "^5.3.18",
    "@storybook/addon-actions": "^5.3.18",
    "@storybook/addon-storyshots": "^5.3.18",
    "@storybook/vue": "^5.3.18",
    "@types/jest": "^24.0.19",
    "@typescript-eslint/eslint-plugin": "^2.27.0",
    "@typescript-eslint/parser": "^2.27.0",
    "@vue/cli-plugin-babel": "~4.3.1",
    "@vue/cli-plugin-eslint": "~4.3.1",
    "@vue/cli-plugin-typescript": "~4.3.1",
    "@vue/cli-plugin-unit-jest": "~4.3.1",
    "@vue/cli-service": "~4.3.1",
    "@vue/composition-api": "^0.5.0",
    "@vue/eslint-config-prettier": "^6.0.0",
    "@vue/eslint-config-typescript": "^5.0.2",
    "@vue/test-utils": "1.0.0-beta.33",
    "eslint": "^6.8.0",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-vue": "^6.2.2",
    "prettier": "^2.0.4",
    "rollup": "^2.4.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-replace": "^2.2.0",
    "rollup-plugin-terser": "^5.3.0",
    "rollup-plugin-typescript2": "^0.27.0",
    "storybook-vue-router": "^1.0.7",
    "typescript": "~3.8.3",
    "vue": "^2.6.11",
    "vue-cli-plugin-storybook": "~1.2.2",
    "vue-cli-plugin-tailwind": "~1.2.0",
    "vue-functional-data-merge": "^3.1.0",
    "vue-router": "^3.1.6",
    "vue-template-compiler": "^2.6.11"
  },
  "peerDependencies": {
    "@vue/composition-api": "^0.5.0",
    "vue": ">= 2.6"
  }
}
