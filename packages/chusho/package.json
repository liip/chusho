{
  "name": "chusho",
  "version": "0.0.0",
  "license": "MIT",
  "description": "A library of bare accessible Vue components and tools.",
  "keywords": [
    "vue",
    "components",
    "accessibility"
  ],
  "author": {
    "name": "Liip",
    "url": "https://www.liip.ch/"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/liip/chusho.git",
    "directory": "packages/chusho"
  },
  "bugs": {
    "url": "https://github.com/liip/chusho/issues"
  },
  "homepage": "https://www.chusho.dev/",
  "scripts": {
    "start": "npm run storybook:serve",
    "storybook:build": "vue-cli-service storybook:build -c config/storybook -s public -o docs --docs",
    "storybook:serve": "vue-cli-service storybook:serve -p 6006 -c config/storybook -s public --ci",
    "storybook:build:debug": "npm run storybook:build -- --debug-webpack",
    "format": "vue-cli-service lint",
    "validate": "vue-cli-service lint --no-fix",
    "test:unit": "vue-cli-service test:unit",
    "test:unit:dev": "vue-cli-service test:unit --watch",
    "test:unit:debug": "node --inspect-brk ./node_modules/jest/bin/jest.js --runInBand",
    "test": "npm run validate && npm run test:unit",
    "build": "vue-cli-service build --target lib --name chusho ./src/main.ts"
  },
  "main": "./dist/chusho.common.js",
  "devDependencies": {
    "@egoist/vue-to-react": "^1.1.0",
    "@storybook/addon-docs": "^5.3.17",
    "@storybook/addon-storyshots": "^5.3.17",
    "@storybook/vue": "^5.3.17",
    "@types/jest": "^24.0.19",
    "@typescript-eslint/eslint-plugin": "^2.24.0",
    "@typescript-eslint/parser": "^2.24.0",
    "@vue/cli-plugin-babel": "~4.2.3",
    "@vue/cli-plugin-eslint": "~4.2.3",
    "@vue/cli-plugin-typescript": "~4.2.3",
    "@vue/cli-plugin-unit-jest": "~4.2.3",
    "@vue/cli-service": "~4.2.3",
    "@vue/composition-api": "^0.5.0",
    "@vue/eslint-config-prettier": "^6.0.0",
    "@vue/eslint-config-typescript": "^5.0.2",
    "@vue/test-utils": "1.0.0-beta.32",
    "chusho-preset-tailwind": "0.0.0",
    "eslint": "^6.8.0",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-vue": "^6.2.2",
    "prettier": "^2.0.1",
    "typescript": "~3.8.3",
    "vue": "^2.6.11",
    "vue-cli-plugin-storybook": "~1.2.1",
    "vue-cli-plugin-tailwind": "~1.2.0",
    "vue-template-compiler": "^2.6.11"
  },
  "peerDependencies": {
    "vue": ">= 2.6"
  },
  "dependencies": {
    "vue-functional-data-merge": "^3.1.0"
  }
}
