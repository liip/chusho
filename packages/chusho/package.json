{
  "name": "chusho",
  "version": "0.1.0-alpha.3",
  "license": "MIT",
  "description": "A library of bare accessible Vue components and tools.",
  "keywords": [
    "vue",
    "components",
    "accessibility",
    "wcag"
  ],
  "author": {
    "name": "Liip",
    "url": "https://www.liip.ch/"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/liip/chusho.git",
    "directory": "packages/chusho"
  },
  "bugs": {
    "url": "https://github.com/liip/chusho/issues"
  },
  "homepage": "https://www.chusho.dev/",
  "scripts": {
    "start": "npm run storybook:serve",
    "storybook:build": "vue-cli-service storybook:build -c config/storybook -s public -o docs --docs",
    "storybook:serve": "vue-cli-service storybook:serve -p 6006 -c config/storybook -s public --ci",
    "storybook:build:debug": "npm run storybook:build -- --debug-webpack",
    "format": "vue-cli-service lint",
    "validate": "vue-cli-service lint --no-fix",
    "test:unit": "vue-cli-service test:unit",
    "test:unit:dev": "vue-cli-service test:unit --watch",
    "test:unit:debug": "node --inspect-brk ./node_modules/jest/bin/jest.js --runInBand",
    "test:e2e:dev": "vue-cli-service test:e2e --url http://localhost:6006",
    "test:e2e:headless": "npm run test:e2e:dev -- --headless",
    "test:e2e": "start-server-and-test start http://localhost:6006 test:e2e:headless",
    "test": "npm run validate && npm run test:unit && npm run test:e2e",
    "build": "rimraf dist && rollup -c",
    "build:watch": "npm run build -- -w",
    "prepublishOnly": "npm run build"
  },
  "main": "dist/chusho.js",
  "umd:main": "dist/chusho.umd.js",
  "module": "dist/chusho.module.js",
  "typings": "dist/chusho.d.ts",
  "sideEffects": false,
  "files": [
    "dist/"
  ],
  "dependencies": {
    "portal-vue": "^2.1.7",
    "ts-essentials": "^7.0.0",
    "vue-client-only": "^2.0.0"
  },
  "devDependencies": {
    "@storybook/addon-a11y": "^5.3.19",
    "@storybook/addon-actions": "^5.3.19",
    "@storybook/vue": "^5.3.19",
    "@types/jest": "^24.0.19",
    "@typescript-eslint/eslint-plugin": "^3.4.0",
    "@typescript-eslint/parser": "^3.4.0",
    "@vue/cli-plugin-babel": "~4.4.6",
    "@vue/cli-plugin-e2e-cypress": "^4.4.6",
    "@vue/cli-plugin-eslint": "~4.4.6",
    "@vue/cli-plugin-typescript": "~4.4.6",
    "@vue/cli-plugin-unit-jest": "~4.4.6",
    "@vue/cli-service": "~4.4.6",
    "@vue/composition-api": "^0.6.7",
    "@vue/eslint-config-prettier": "^6.0.0",
    "@vue/eslint-config-typescript": "^5.0.2",
    "@vue/test-utils": "1.0.3",
    "eslint": "^7.3.1",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-vue": "^6.2.2",
    "prettier": "^2.0.5",
    "rimraf": "^3.0.2",
    "rollup": "^2.18.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-replace": "^2.2.0",
    "rollup-plugin-terser": "^6.1.0",
    "rollup-plugin-typescript2": "^0.27.1",
    "start-server-and-test": "^1.11.0",
    "storybook-vue-router": "^1.0.7",
    "typescript": "~3.9.5",
    "vue": "^2.6.11",
    "vue-cli-plugin-storybook": "~1.2.2",
    "vue-cli-plugin-tailwind": "~1.4.1",
    "vue-functional-data-merge": "^3.1.0",
    "vue-router": "^3.3.4",
    "vue-template-compiler": "^2.6.11"
  },
  "peerDependencies": {
    "@vue/composition-api": "^0.6.7",
    "vue": ">= 2.6"
  }
}
